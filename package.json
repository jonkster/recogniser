{
  "name": "recogniser",
  "version": "1.0.0",
  "description": "A simple handwriting recogniser using a neural net.",
  "scripts": {
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" \"node ws/recogniser.js\" ",
    "lite": "lite-server",
    "postinstall": "typings install",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonkster/recogniser.git"
  },
  "author": "Jonathan Kelly",
  "license": "0BSD",
  "dependencies": {
    "bootstrap": "^3.3.6",
    "convnetjs": "^0.3.0",
    "es6-shim": "^0.35.0",
    "reflect-metadata": "^0.1.3",
    "restify": "^4.0.4",
    "sprintf-js": "^1.0.3",
    "systemjs": "0.19.27",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "concurrently": "^2.0.0",
    "lite-server": "^2.2.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "bugs": {
    "url": "https://github.com/jonkster/recogniser/issues"
  },
  "homepage": "https://github.com/jonkster/recogniser#readme"
